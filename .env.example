# This is an example file. Copy it to .env and fill in your actual values.

# Agent Keys
GOOGLE_API_KEY="YOUR_GOOGLE_API_KEY"
LANGFUSE_PUBLIC_KEY="YOUR_LANGFUSE_PUBLIC_KEY"
LANGFUSE_SECRET_KEY="YOUR_LANGFUSE_SECRET_KEY"

# Langfuse Database & Services Secrets
# We recommend generating secure random passwords for production.
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=postgres

CLICKHOUSE_USER=clickhouse
CLICKHOUSE_PASSWORD=clickhouse

MINIO_ROOT_USER=minio
MINIO_ROOT_PASSWORD=miniosecret

REDIS_PASSWORD=myredissecret

# Langfuse Application Secrets
# Generate a random string for these secrets
SALT="a_random_salt_string"
ENCRYPTION_KEY="a_32_byte_random_hex_string_for_encryption" # e.g., generate via `openssl rand -hex 32`
NEXTAUTH_SECRET="a_random_string_for_nextauth"

# Optional: Define a specific postgres version
# POSTGRES_VERSION=latest
